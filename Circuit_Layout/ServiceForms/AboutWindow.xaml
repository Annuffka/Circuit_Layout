<Window x:Class="Circuit_Layout.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:res="clr-namespace:Circuit_Layout.Properties"
        Title="{x:Static res:Resources.aw_title}" Height="190" Width="360" ResizeMode="NoResize" Icon="/Resources/icon.ico" Background="#9acf5c">
    <DockPanel>
        <StackPanel DockPanel.Dock="Top" Height="120" Orientation="Horizontal">
            <StackPanel Width="224">
                <TextBlock Margin="5" Text="Circuit Layout v1.0" HorizontalAlignment="Center" FontWeight="Bold"/>
                <TextBlock Margin="10 0 5 0" Text="{x:Static res:Resources.aw_dev}"/>
                <TextBlock Margin="25 0 5 0" Text="{x:Static res:Resources.aw_nazarova}"/>
                <TextBlock Margin="25 0 5 0" Text="{x:Static res:Resources.aw_sorvanova}"/>
                <TextBlock Margin="25 0 5 0" Text="{x:Static res:Resources.aw_danilov}"/>
                <TextBlock Margin="25 0 5 0" Text="{x:Static res:Resources.aw_anufriev}"/>
            </StackPanel>

            <Image Source="../Resources/logoSmall.png" Width="120" Margin="10 10 10 10"/>
        </StackPanel>
        <Button DockPanel.Dock="Bottom" Content="OK" HorizontalAlignment="Center" Width="140" Height="21" Click="Button_Click"></Button>
    </DockPanel>
</Window>
